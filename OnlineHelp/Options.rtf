\par }\pard\plain \s1\tx115\li115\ri130\sb120\sa80\widctlpar\outlinelevel0\adjustright \b\f1\fs18\kerning28\cgrid {\page }{\cs18\super #{\footnote \pard\plain \s20\tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs16\cgrid {\cs18\fs18\super #}{\fs18  HIDS_INI_TASKSTATUS_SHOWCOL_PERSONA}}}{ }{\cs18\super ${\footnote \pard\plain \s20\tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs16\cgrid {\cs18\fs18\super $}{\fs18  Persona (Task Status)}}}{ }{\cs18\super K{\footnote \pard\plain \s20\tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs16\cgrid {\cs18\fs18\super K}{\fs18  Persona (Task Status)}}}{ }{\cs18\super @{\footnote \pard\plain \s20\tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs16\cgrid {\cs18\fs18\super @}{\fs18  Status|0|0|0|0||||||}}}{ }{\cs18\super +{\footnote \pard\plain \s20\tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs16\cgrid {\cs18\fs18\super +}{\fs18  OPTIONS:0}}}{ Persona (Task Status)
