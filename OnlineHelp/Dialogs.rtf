\par }\pard\plain \s1\tx115\li115\ri130\sb120\sa80\widctlpar\outlinelevel0\adjustright \b\f1\fs18\kerning28\cgrid {\page }{\cs15\super #{\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\f1\fs18\super #}{\f1\fs18  HIDR_FIND}}K{\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\f1\fs18\super K}{\f1\fs18  Find dialog}} ${\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\f1\fs18\super $}{\f1\fs18  Find dialog}} }{Find dialog
\par }\pard\plain \tx115\li115\ri130\sa48\widctlpar\adjustright \f1\fs18\cgrid {Use this dialog box to find a character string within one message.
\par }\pard\plain \s1\tx115\li115\ri130\sb120\sa80\widctlpar\outlinelevel0\adjustright \b\f1\fs18\kerning28\cgrid {\page }{\cs15\super #{\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\super #}{ HID_EDIT_FIND_BEGIN}}}{ }{\cs15\super ${\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\super $}{ Find}}}{ }{\cs15\super K{\footnote \pard\plain \s19\tx120\li120\sb80\sa48\sl-240\slmult0\nowidctlpar\adjustright \f11\fs20\cgrid {\cs15\super K}{ Find (in Find dialog box)}}}{ Find
